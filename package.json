        "@types/node": "^13.13.47",
        "@types/request": "^2.48.5",
        "axios": "^0.22.0",
        "bcrypt": "^4.0.0",
        "chance": "^1.1.7",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "express": "^4.17.1",
        "fcm-node": "^1.5.2",
        "form-data": "^3.0.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "mongodb": "^3.6.5",
        "multer": "^1.4.2",
        "pg": "8.5.1",
        "reflect-metadata": "^0.1.13",
